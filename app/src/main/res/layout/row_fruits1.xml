<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="fruits1RowModel" type="com.glosary.app.modules.fruits.data.model.Fruits1RowModel"/>
    </data>
    <LinearLayout android:gravity="end" android:id="@+id/linearGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_102cornerRadius">
        <ImageView android:id="@+id/imageIcon"
            android:layout_height="@dimen/_26pxh"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_156pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="@dimen/_25pxh"
            android:scaleType="fitXY" android:src="@drawable/img_icon_25"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearFrame"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_79pxh"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_width="@dimen/_107pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image_15}" tools:src="@drawable/img_image_15"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFrame1"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup3313"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFrame2"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtStrawberry"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.73"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{fruits1RowModel.txtStrawberry}"
                            style="@style/txtMontserratbold18" tools:text="@string/lbl_strawberry"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt1Kg"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.27"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{fruits1RowModel.txt1Kg}"
                            style="@style/txtMontserratsemibold14_2" tools:text="@string/lbl_1_kg2"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearFrame3"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt4"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.3"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{fruits1RowModel.txt4}"
                            style="@style/txtMontserratsemibold18_3" tools:text="@string/lbl_4"/>
                        <LinearLayout android:id="@+id/linearFrame4"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_94pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.7"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIcon1"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_icon_26"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txt1"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.14"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{fruits1RowModel.txt1}"
                                style="@style/txtMontserratsemibold18_3" tools:text="@string/lbl_1"/>
                            <ImageView android:id="@+id/imageIcon2"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_icon_27"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearFrame5"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSubscribe"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="@dimen/_82pxh"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_10pxh"
                        android:paddingStart="@dimen/_10pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_subscribe"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_3" tools:text="@string/lbl_subscribe"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnBuyOnce"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_width="@dimen/_78pxh"
                        android:paddingBottom="@dimen/_7pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_9pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_buy_once"
                        android:textAllCaps="false"
                        style="@style/btnRoundedOutline_1" tools:text="@string/lbl_buy_once"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
