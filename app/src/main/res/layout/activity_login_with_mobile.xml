<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginWithMobileVM" type="com.glosary.app.modules.loginwithmobile.data.viewmodel.LoginWithMobileVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearLoginwithMobi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearLogo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageGroup400"
                android:layout_height="@dimen/_53pxh"
                android:layout_width="@dimen/_36pxh"
                android:scaleType="fitXY" android:src="@drawable/img_group400"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSGGrocery"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.83" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginWithMobileVM.loginWithMobileModel.txtSGGrocery}"
                style="@style/txtMontserratlight28" tools:text="@string/lbl_sg_grocery"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageIllustrator"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_304pxh"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_50pxv"
            android:layout_width="@dimen/_313pxh"
            android:scaleType="fitXY" android:src="@drawable/img_illustrator"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtLoginRegist"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_57pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_91pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{loginWithMobileVM.loginWithMobileModel.txtLoginRegist}"
            style="@style/txtMontserratsemibold24" tools:text="@string/msg_login_regist"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtMobileNumber"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_268pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{loginWithMobileVM.loginWithMobileModel.txtMobileNumber}"
            style="@style/txtMontserratmedium18" tools:text="@string/lbl_mobile_number"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup716"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_5">
            <ImageView android:id="@+id/imageImage61"
                android:layout_height="@dimen/_27pxh"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_26pxh"
                android:scaleType="fitXY" android:src="@drawable/img_image61"/>
            <ImageView android:id="@+id/imageIcCaretdown"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY" android:src="@drawable/img_iccaretdown"/>
            <TextView android:ellipsize="end" android:id="@+id/txt1"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.06" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginWithMobileVM.loginWithMobileModel.txt1}"
                style="@style/txtMontserratregular18_7" tools:text="@string/lbl_19"/>
            <View android:background="@color/gray_501"
                android:id="@+id/lineLine3"
                android:layout_height="@dimen/_46pxv"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.0" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEnterYourMobi"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_65pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.75" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginWithMobileVM.loginWithMobileModel.txtEnterYourMobi}"
                style="@style/txtMontserratregular16_7" tools:text="@string/msg_enter_your_mobi"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnGetOTP"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_get_otp"
            android:textAllCaps="false" style="@style/btnSolidRounded_6" tools:text="@string/lbl_get_otp"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearOrcontinuewit"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View android:background="@color/bluegray_402"
                android:id="@+id/lineLine2"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_weight="0.32" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrContinueWit"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_weight="0.36" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginWithMobileVM.loginWithMobileModel.txtOrContinueWit}"
                style="@style/txtMontserratregular16_7" tools:text="@string/msg_or_continue_wit"/>
            <View android:background="@color/bluegray_402"
                android:id="@+id/lineLine1"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_weight="0.32" android:layout_width="0dp"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearFrame"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_32pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearGroup717"
                android:layout_height="wrap_content"
                android:layout_weight="0.49" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_54"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGoogle"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.67"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginWithMobileVM.loginWithMobileModel.txtGoogle}"
                    style="@style/txtMontserratmedium18_1" tools:text="@string/lbl_google"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearGroup718"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_weight="0.51" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_55"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFacebook"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.74"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginWithMobileVM.loginWithMobileModel.txtFacebook}"
                    style="@style/txtMontserratmedium18_1" tools:text="@string/lbl_facebook"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
