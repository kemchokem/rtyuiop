<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="paymentAddNewAddressVM" type="com.glosary.app.modules.paymentaddnewaddress.data.viewmodel.PaymentAddNewAddressVM"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup671"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegreen_500">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="bottom|start"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameGroup661"
                android:layout_height="@dimen/_744pxv" android:layout_width="match_parent">
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_tl_30_tr_30"
                    android:id="@+id/viewRectangle1242"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_744pxh" android:layout_width="@dimen/_428pxh"/>
                <LinearLayout android:id="@+id/linearGroup664"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_vector_1"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtAddNewAddress"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{paymentAddNewAddressVM.paymentAddNewAddressModel.txtAddNewAddress}"
                        style="@style/txtMontserratsemibold22" tools:text="@string/lbl_add_new_address"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_100_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_country"
                        android:id="@+id/etCountry"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_22pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@={paymentAddNewAddressVM.paymentAddNewAddressModel.etCountryValue}"
                        android:textColorHint="@color/gray_500" style="@style/etSolidRounded"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_100_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_state"
                        android:id="@+id/etState"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_22pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@={paymentAddNewAddressVM.paymentAddNewAddressModel.etStateValue}"
                        android:textColorHint="@color/gray_500" style="@style/etSolidRounded"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_100_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_city"
                        android:id="@+id/etCity"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_22pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@={paymentAddNewAddressVM.paymentAddNewAddressModel.etCityValue}"
                        android:textColorHint="@color/gray_500" style="@style/etSolidRounded"/>
                    <TextView android:gravity="start"
                        android:id="@+id/txtPincode"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_21pxv"
                        android:text="@{paymentAddNewAddressVM.paymentAddNewAddressModel.txtPincode}"
                        style="@style/txtSolidRounded" tools:text="@string/lbl_pincode"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_100_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_area"
                        android:id="@+id/etArea"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_22pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@={paymentAddNewAddressVM.paymentAddNewAddressModel.etAreaValue}"
                        android:textColorHint="@color/gray_500" style="@style/etSolidRounded"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_100_radius_10"
                        android:ellipsize="end"
                        android:hint="@string/lbl_landmark"
                        android:id="@+id/etLandmark"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_22pxv"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@={paymentAddNewAddressVM.paymentAddNewAddressModel.etLandmarkValue}"
                        android:textColorHint="@color/gray_500" style="@style/etSolidRounded"/>
                    <FrameLayout android:id="@+id/frameGroup667"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_34pxv"
                        android:layout_marginEnd="@dimen/_38pxh"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_297pxh">
                        <LinearLayout android:id="@+id/linearGroup669"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <androidx.appcompat.widget.AppCompatCheckBox
                                android:buttonTint="@color/gray_505"
                                android:ellipsize="end"
                                android:fontFamily="@font/montserrat"
                                android:id="@+id/checkBoxGroup88"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.51"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@string/lbl_home"
                                android:textColor="@color/gray_500"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="normal" tools:text="@string/lbl_home"/>
                            <androidx.appcompat.widget.AppCompatCheckBox
                                android:buttonTint="@color/gray_505"
                                android:ellipsize="end"
                                android:fontFamily="@font/montserrat"
                                android:id="@+id/checkBoxGroup89"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_160pxh"
                                android:layout_weight="0.49"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@string/lbl_other"
                                android:textColor="@color/gray_500"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_14pxh"
                                android:textStyle="normal" tools:text="@string/lbl_other"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearGroup2835"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <View
                                android:background="@drawable/rectangle_border_gray_505_radius_2"
                                android:id="@+id/viewRectangle1232"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_16pxv"
                                android:layout_marginStart="@dimen/_91pxh" android:layout_width="@dimen/_17pxh"/>
                            <TextView android:id="@+id/txtWorkOffice"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_88pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_weight="0.84"
                                android:layout_width="0dp"
                                android:text="@{paymentAddNewAddressVM.paymentAddNewAddressModel.txtWorkOffice}"
                                style="@style/txtMontserratregular14_10" tools:text="@string/lbl_work_office"/>
                        </LinearLayout>
                    </FrameLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSave"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_199pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_save"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_save"/>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </FrameLayout>
</layout>
