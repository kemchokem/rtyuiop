<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeScreen2RowModel" type="com.glosary.app.modules.homescreen.data.model.HomeScreen2RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup"
        android:layout_height="wrap_content" android:layout_width="@dimen/_87pxh">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_88pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_87pxh"
            android:scaleType="fitXY" android:src="@drawable/img_background_3"/>
        <LinearLayout android:id="@+id/linearFrame2"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_65pxh"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="@dimen/_75pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image_52}" tools:src="@drawable/img_image_52"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnGroecries"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_14pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_groecries"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedBottomCorner" tools:text="@string/lbl_groecries"/>
        </LinearLayout>
    </FrameLayout>
</layout>
