<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myAddresses1RowModel" type="com.glosary.app.modules.myaddresses.data.model.MyAddresses1RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearFrame"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageSelect"
            android:layout_height="@dimen/_18pxh"
            android:layout_marginBottom="@dimen/_106pxv"
            android:layout_marginTop="@dimen/_50pxv"
            android:layout_width="@dimen/_18pxh"
            android:scaleType="fitXY" android:src="@drawable/img_select"/>
        <FrameLayout android:id="@+id/frameGroup311"
            android:layout_height="@dimen/_174pxv"
            android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_362pxh">
            <ImageView android:id="@+id/imageRectangle56"
                android:layout_gravity="start"
                android:layout_height="@dimen/_174pxh"
                android:layout_width="@dimen/_362pxh"
                android:scaleType="fitXY" android:src="@drawable/img_rectangle56"/>
            <LinearLayout android:id="@+id/linearFrame1"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_24pxh"
                android:layout_marginVertical="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearGroup308"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFrame2"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIcon"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_icon_13"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHome"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_weight="0.74"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{myAddresses1RowModel.txtHome}"
                            style="@style/txtMontserratsemibold18_2" tools:text="@string/lbl_home"/>
                    </LinearLayout>
                    <TextView android:gravity="start"
                        android:id="@+id/txt4517Washington"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:text="@{myAddresses1RowModel.txt4517Washington}"
                        style="@style/txtMontserratregular14_5" tools:text="@string/msg_4517_washington"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageFrame"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_72pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_width="@dimen/_52pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_frame_13"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
