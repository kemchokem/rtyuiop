<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myCart1RowModel" type="com.glosary.app.modules.mycart.data.model.MyCart1RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageCard"
            android:layout_gravity="start"
            android:layout_height="@dimen/_119pxh"
            android:layout_width="@dimen/_396pxh"
            android:scaleType="fitXY" android:src="@drawable/img_card"/>
        <LinearLayout android:id="@+id/linearGroup193"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <FrameLayout android:id="@+id/frameGroup233"
                android:layout_height="@dimen/_89pxv"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_89pxh">
                <ImageView android:id="@+id/imageFrame"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_89pxh"
                    android:layout_width="@dimen/_89pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_frame"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="@dimen/_57pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_image"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFrame"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_weight="0.39" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtShimlaApple"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{myCart1RowModel.txtShimlaApple}"
                    style="@style/txtMontserratregular18" tools:text="@string/lbl_shimla_apple"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt1Kg"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{myCart1RowModel.txt1Kg}"
                    style="@style/txtMontserratregular14" tools:text="@string/lbl_1_kg"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup3400"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt2"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.31"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{myCart1RowModel.txt2}"
                        style="@style/txtHelveticabold18" tools:text="@string/lbl_2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt325"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_weight="0.69"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{myCart1RowModel.txt325}"
                        style="@style/txtHelvetica18" tools:text="@string/lbl_3_25"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearGroup194"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_weight="0.32" android:layout_width="0dp" android:orientation="vertical">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_1"/>
                <LinearLayout android:id="@+id/linearFrame2"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_53pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIcon1"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.14"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{myCart1RowModel.txt1}"
                        style="@style/txtMontserratsemibold24" tools:text="@string/lbl_1"/>
                    <ImageView android:id="@+id/imageIcon2"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_3"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
