<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="privacyPolicyVM" type="com.glosary.app.modules.privacypolicy.data.viewmodel.PrivacyPolicyVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearGroup410"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_500">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFrame"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_222pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_6"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrivacyPolicy"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{privacyPolicyVM.privacyPolicyModel.txtPrivacyPolicy}"
                    style="@style/txtMontserratbold22_1" tools:text="@string/lbl_privacy_policy"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup405"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrivacyPolicy1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{privacyPolicyVM.privacyPolicyModel.txtPrivacyPolicy1}"
                    style="@style/txtMontserratsemibold22_1" tools:text="@string/lbl_privacy_policy"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLastUpdatedOn"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{privacyPolicyVM.privacyPolicyModel.txtLastUpdatedOn}"
                    style="@style/txtMontserratregular16_3" tools:text="@string/msg_last_updated_on"/>
                <FrameLayout android:id="@+id/frameGroup408"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="2003dp"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_396pxh">
                    <TextView android:id="@+id/txtInformationCol"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_489pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_489pxv"
                        android:layout_width="match_parent"
                        android:text="@{privacyPolicyVM.privacyPolicyModel.txtInformationCol}"
                        style="@style/txtMontserratregular14_5" tools:text="@string/msg_information_col"/>
                    <LinearLayout android:id="@+id/linearText"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:id="@+id/txtSGGroceryIsC"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtSGGroceryIsC}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_sg_grocery_is_c"/>
                        <TextView android:id="@+id/txtSGGroceryColl"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_51pxv"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtSGGroceryColl}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_sg_grocery_coll"/>
                        <TextView android:id="@+id/txtUseOfYourInf"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtUseOfYourInf}"
                            style="@style/txtMontserratsemibold18_2" tools:text="@string/msg_use_of_your_inf"/>
                        <TextView android:id="@+id/txtWeWillUseYo"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtWeWillUseYo}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_we_will_use_you"/>
                        <TextView android:id="@+id/txtLegalDisclaime"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtLegalDisclaime}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_legal_disclaime"/>
                        <TextView android:id="@+id/txtWeReserveThe"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtWeReserveThe}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_we_reserve_the"/>
                        <TextView android:id="@+id/txtChangesInThis"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtChangesInThis}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_changes_in_this"/>
                        <TextView android:id="@+id/txtSGGroceryRese"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{privacyPolicyVM.privacyPolicyModel.txtSGGroceryRese}"
                            style="@style/txtMontserratregular14_5" tools:text="@string/msg_sg_grocery_rese"/>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearTabbar"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_137pxv"
                    android:layout_width="match_parent" android:orientation="vertical"/>
            </LinearLayout>
        </ScrollView>
        <FrameLayout android:id="@+id/frameTabbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegreen_500">
            <LinearLayout android:id="@+id/linearGroup39"
                android:layout_gravity="top|center"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon1"/>
                <ImageView android:id="@+id/imageIocn2"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_iocn2"/>
                <ImageView android:id="@+id/imageIcon3"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon3_7"/>
                <ImageView android:id="@+id/imageIcon4"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon4_2"/>
                <ImageView android:id="@+id/imageIcon5"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon5"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
