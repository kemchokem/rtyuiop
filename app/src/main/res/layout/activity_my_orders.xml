<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myOrdersVM" type="com.glosary.app.modules.myorders.data.viewmodel.MyOrdersVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearGroup358"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegreen_500">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearFrame"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearGroup360"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_6"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMyOrders"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{myOrdersVM.myOrdersModel.txtMyOrders}"
                        style="@style/txtMontserratbold22_1" tools:text="@string/lbl_my_orders2"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageIcon1"
                    android:layout_height="@dimen/_23pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_222pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_15"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameGroup347"
                android:layout_height="@dimen/_854pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearGroup344"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOrderHistory"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{myOrdersVM.myOrdersModel.txtOrderHistory}"
                        style="@style/txtMontserratsemibold22_1" tools:text="@string/lbl_order_history"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup337"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_138pxv"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout android:id="@+id/frameGroup349"
                            android:layout_height="@dimen/_626pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_6pxv" android:layout_width="@dimen/_18pxh">
                            <View android:background="@color/green_500"
                                android:id="@+id/lineLine"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_617pxv"
                                android:layout_marginEnd="@dimen/_8pxh"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_2pxh"/>
                            <ImageView android:id="@+id/imageFrame"
                                android:layout_gravity="top|start"
                                android:layout_height="@dimen/_443pxv"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY" android:src="@drawable/img_frame_16"/>
                        </FrameLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearFrame1"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_45pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_weight="0.95"
                            android:layout_width="0dp" android:orientation="vertical">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearItem1"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="vertical">
                                <LinearLayout android:gravity="start"
                                    android:id="@+id/linearFrame2"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal" style="@style/groupStylewhite_3">
                                    <ImageView
                                    android:id="@+id/imageImage"
                                    android:layout_height="@dimen/_48pxh"
                                    android:layout_marginBottom="@dimen/_50pxv"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:layout_marginTop="@dimen/_44pxv"
                                    android:layout_width="@dimen/_57pxh"
                                    android:scaleType="fitXY" android:src="@drawable/img_image_4"/>
                                    <FrameLayout
                                    android:id="@+id/frameGroup351"
                                    android:layout_height="@dimen/_55pxv"
                                    android:layout_marginBottom="@dimen/_46pxv"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_41pxv" android:layout_width="@dimen/_53pxh">
                                    <ImageView
                                    android:id="@+id/imageImage1"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_54pxh"
                                    android:layout_width="@dimen/_52pxh"
                                    android:scaleType="fitXY"
                                    imageRes="@{@drawable/img_image_5}" tools:src="@drawable/img_image_5"/>

                                    <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btn1"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:paddingBottom="@dimen/_12pxv"
                                    android:paddingEnd="@dimen/_13pxh"
                                    android:paddingStart="@dimen/_14pxh"
                                    android:paddingTop="@dimen/_11pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_13"
                                    android:textAllCaps="false"
                                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_13"/>
                                    </FrameLayout>
                                    <LinearLayout
                                    android:gravity="start"
                                    android:id="@+id/linearFrame3"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_50pxv"
                                    android:layout_marginEnd="@dimen/_52pxh"
                                    android:layout_marginStart="@dimen/_27pxh"
                                    android:layout_marginTop="@dimen/_36pxv"
                                    android:layout_weight="0.57"
                                    android:layout_width="0dp" android:orientation="vertical">
                                    <TextView
                                    android:ellipsize="end"
                                    android:id="@+id/txtArrivesTomorro"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{myOrdersVM.myOrdersModel.txtArrivesTomorro}"
                                    style="@style/txtMontserratsemibold16_2" tools:text="@string/msg_arrives_tomorro"/>
                                    <TextView
                                    android:ellipsize="end"
                                    android:id="@+id/txt7AMPM"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{myOrdersVM.myOrdersModel.txt7AMPM}"
                                    style="@style/txtMontserratregular16_2" tools:text="@string/lbl_7_am_pm"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout android:gravity="start"
                                    android:id="@+id/linearGroup328"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent" android:orientation="vertical">
                                    <LinearLayout
                                    android:id="@+id/linearGroup326"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent" android:orientation="horizontal">

                                    <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnViewOrderDeta"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.55"
                                    android:layout_width="0dp"
                                    android:paddingBottom="@dimen/_10pxv"
                                    android:paddingEnd="@dimen/_7pxh"
                                    android:paddingStart="@dimen/_7pxh"
                                    android:paddingTop="@dimen/_9pxv"
                                    android:singleLine="true"
                                    android:text="@string/msg_view_order_deta"
                                    android:textAllCaps="false"
                                    style="@style/btnRoundedOutline" tools:text="@string/msg_view_order_deta"/>

                                    <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnTrackOrder"
                                    android:layout_height="@dimen/_36pxv"
                                    android:layout_marginStart="@dimen/_102pxh"
                                    android:layout_width="@dimen/_120pxh"
                                    android:paddingBottom="@dimen/_10pxv"
                                    android:paddingEnd="@dimen/_17pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_9pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_track_order"
                                    android:textAllCaps="false"
                                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_track_order"/>
                                    </LinearLayout>
                                    <LinearLayout
                                    android:id="@+id/linearGroup327"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent" android:orientation="horizontal">
                                    <TextView
                                    android:ellipsize="end"
                                    android:id="@+id/txtEditOrder"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.48"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{myOrdersVM.myOrdersModel.txtEditOrder}"
                                    style="@style/txtMontserratsemibold14" tools:text="@string/lbl_edit_order"/>
                                    <TextView
                                    android:ellipsize="end"
                                    android:id="@+id/txtGetInvoice"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_212pxh"
                                    android:layout_weight="0.52"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{myOrdersVM.myOrdersModel.txtGetInvoice}"
                                    style="@style/txtMontserratsemibold14" tools:text="@string/lbl_get_invoice"/>
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>
                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recyclerMyOrders"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_24pxv"
                                android:layout_width="match_parent"
                                android:nestedScrollingEnabled="false"
                                android:orientation="vertical"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                itemSpace="@{@dimen/_24pxh}"
                                tools:itemCount="2" tools:listitem="@layout/row_my_orders1"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTabbar"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical"/>
            </FrameLayout>
        </androidx.core.widget.NestedScrollView>
        <FrameLayout android:id="@+id/frameTabbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegreen_500">
            <LinearLayout android:id="@+id/linearGroup28"
                android:layout_gravity="top|center"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon2"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon1_1"/>
                <ImageView android:id="@+id/imageIocn2"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_iocn2"/>
                <ImageView android:id="@+id/imageIcon3"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon3_4"/>
                <ImageView android:id="@+id/imageIcon4"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon4_1"/>
                <ImageView android:id="@+id/imageIcon5"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon5"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
