<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mySubscriptionVM" type="com.glosary.app.modules.mysubscription.data.viewmodel.MySubscriptionVM"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup617"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageGreenbackgrou"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" android:src="@drawable/img_greenbackgrou"/>
        <LinearLayout android:id="@+id/linearGroup616"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearGroup582"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearGroup6020"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearFrame"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.89"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageIcon"
                                android:layout_height="@dimen/_26pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_icon_6"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtMySubscription"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_weight="0.94"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{mySubscriptionVM.mySubscriptionModel.txtMySubscription}"
                                style="@style/txtMontserratbold22_1" tools:text="@string/lbl_my_subscription"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageIcon1"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_154pxh"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_icon_47"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <FrameLayout android:id="@+id/frameGroup611"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_51pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_30pxv" android:layout_width="@dimen/_396pxh">
                    <ImageView android:id="@+id/imageCard"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_51pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY" android:src="@drawable/img_card_8"/>
                    <LinearLayout android:id="@+id/linearFrame1"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSubscriptionW"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.92"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{mySubscriptionVM.mySubscriptionModel.txtSubscriptionW}"
                            style="@style/txtMontserratsemibold16_4" tools:text="@string/msg_subscription_w"/>
                        <ImageView android:id="@+id/imageIcon2"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_158pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_icon_48"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
            <androidx.core.widget.NestedScrollView
                android:clipToPadding="false"
                android:fillViewport="true"
                android:id="@+id/scrollViewScrollview"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_weight="1" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearGroup581"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearFrame2"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtToday"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.22"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{mySubscriptionVM.mySubscriptionModel.txtToday}"
                            style="@style/txtMontserratbold18_3" tools:text="@string/lbl_today"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt23September2"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.69"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{mySubscriptionVM.mySubscriptionModel.txt23September2}"
                            style="@style/txtMontserratregular18_6" tools:text="@string/msg_23_september_2"/>
                        <ImageView android:id="@+id/imageIcon3"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_icon_49"/>
                    </LinearLayout>
                    <androidx.recyclerview.widget.RecyclerView
                        android:gravity="center_horizontal"
                        android:id="@+id/recyclerMySubscription"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_15pxh}" tools:itemCount="3" tools:listitem="@layout/row_my_subscription1"/>
                    <ImageView android:id="@+id/imageIllustration"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_88pxh"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_width="@dimen/_216pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_illustration"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtExcitedToServ"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mySubscriptionVM.mySubscriptionModel.txtExcitedToServ}"
                        style="@style/txtMontserratregular15" tools:text="@string/msg_excited_to_serv"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTabbar"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="match_parent" android:orientation="vertical"/>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>
            <FrameLayout android:id="@+id/frameTabbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStylegreen_500">
                <LinearLayout android:id="@+id/linearGroup78"
                    android:layout_gravity="top|center"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIcon4"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon1_1"/>
                    <ImageView android:id="@+id/imageIocn2"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_59pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_iocn2"/>
                    <ImageView android:id="@+id/imageIcon5"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon3_2"/>
                    <ImageView android:id="@+id/imageIcon6"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon4_1"/>
                    <ImageView android:id="@+id/imageIcon7"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_59pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon5"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
