<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myOrders1RowModel" type="com.glosary.app.modules.myorders.data.model.MyOrders1RowModel"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearFrame"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_3">
            <LinearLayout android:id="@+id/linearGroup330"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_24pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_weight="0.44" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_height="@dimen/_55pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_59pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image_6}" tools:src="@drawable/img_image_6"/>
                <FrameLayout android:id="@+id/frameGroup353"
                    android:layout_height="@dimen/_55pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_53pxh">
                    <ImageView android:id="@+id/imageSDPAS0023500r7"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_width="@dimen/_52pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_image_5}" tools:src="@drawable/img_image_5"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btn1"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingStart="@dimen/_14pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_14"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_14"/>
                </FrameLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDeliveredOn23"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_42pxv"
                android:layout_marginEnd="@dimen/_39pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_weight="0.56" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{myOrders1RowModel.txtDeliveredOn23}"
                style="@style/txtMontserratsemibold16_2" tools:text="@string/msg_delivered_on_23"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearGroup331"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnViewOrderDeta"
                android:layout_height="wrap_content"
                android:layout_weight="0.64" android:layout_width="0dp"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_7pxh"
                android:paddingStart="@dimen/_7pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/msg_view_order_deta"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline" tools:text="@string/msg_view_order_deta"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGetInvoice"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_9pxv"
                android:layout_marginStart="@dimen/_141pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.36" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{myOrders1RowModel.txtGetInvoice}"
                style="@style/txtMontserratsemibold14" tools:text="@string/lbl_get_invoice"/>
        </LinearLayout>
    </LinearLayout>
</layout>
